<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.WebView?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            prefHeight="700.0" prefWidth="1300.0" styleClass="mainRoot"
            stylesheets="@../../css/mainStyle.css" xmlns="http://javafx.com/javafx/8"
            fx:controller="sepm.ss15.grp16.gui.controller.main.MainController">
    <center>
        <VBox>
            <children>
                <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="mainBox"
                      VBox.vgrow="ALWAYS">
                    <children>
                        <VBox prefHeight="315.0">
                            <children>
                                <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                                    <children>
                                        <ImageView fx:id="userImgView" fitHeight="280.0" fitWidth="270.0"
                                                   pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../../icons/fat_to_muscle.png"/>
                                            </image>
                                            <viewport>
                                                <Rectangle2D/>
                                            </viewport>
                                            <HBox.margin>
                                                <Insets right="20.0"/>
                                            </HBox.margin>
                                        </ImageView>
                                        <GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES"/>
                                                <ColumnConstraints hgrow="SOMETIMES"/>
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                            </rowConstraints>
                                            <children>
                                                <Label minWidth="130.0" prefWidth="130.0" text="Geschlecht:">
                                                    <font>
                                                        <Font size="17.0"/>
                                                    </font>
                                                </Label>
                                                <Label fx:id="genderTextField" text="Label" GridPane.columnIndex="1"/>
                                                <Label text="Alter:" GridPane.rowIndex="1">
                                                    <font>
                                                        <Font size="17.0"/>
                                                    </font>
                                                </Label>
                                                <Label fx:id="ageTextField" text="Label" GridPane.columnIndex="1"
                                                       GridPane.rowIndex="1"/>
                                                <Label text="Größe:" GridPane.rowIndex="2">
                                                    <font>
                                                        <Font size="17.0"/>
                                                    </font>
                                                </Label>
                                                <Label fx:id="heightTextField" text="Label" GridPane.columnIndex="1"
                                                       GridPane.rowIndex="2"/>
                                                <Label text="Gewicht:" GridPane.rowIndex="3">
                                                    <font>
                                                        <Font size="17.0"/>
                                                    </font>
                                                </Label>
                                                <Label fx:id="weightTextField" text="Label" GridPane.columnIndex="1"
                                                       GridPane.rowIndex="3"/>
                                                <Label minWidth="150.0" prefWidth="150.0" text="Körperfett:"
                                                       GridPane.rowIndex="4">
                                                    <font>
                                                        <Font size="17.0"/>
                                                    </font>
                                                </Label>
                                                <VBox maxHeight="1.7976931348623157E308"
                                                      maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"
                                                      GridPane.rowIndex="4">
                                                    <children>
                                                        <HBox maxHeight="1.7976931348623157E308"
                                                              maxWidth="1.7976931348623157E308" spacing="15.0"
                                                              GridPane.columnIndex="1" GridPane.halignment="CENTER"
                                                              GridPane.rowIndex="4" GridPane.valignment="CENTER">
                                                            <children>
                                                                <Label fx:id="bodyfatTextField"
                                                                       maxHeight="1.7976931348623157E308"
                                                                       maxWidth="1.7976931348623157E308" text="Label"
                                                                       GridPane.columnIndex="1" GridPane.rowIndex="4">
                                                                    <padding>
                                                                        <Insets top="11.0"/>
                                                                    </padding>
                                                                </Label>
                                                                <Button fx:id="btnBodyfatHelp" minHeight="0.0"
                                                                        minWidth="0.0" mnemonicParsing="false"
                                                                        onAction="#bodyfatHelpClicked" prefHeight="35.0"
                                                                        prefWidth="20.0" styleClass="btnBodyfatHelp"
                                                                        stylesheets="@../../css/mainStyle.css"
                                                                        GridPane.columnIndex="1" GridPane.rowIndex="4">
                                                                    <tooltip>
                                                                        <Tooltip
                                                                                text="Wie eigenen Körperfettanteil messen?"/>
                                                                    </tooltip>
                                                                    <HBox.margin>
                                                                        <Insets top="5.0"/>
                                                                    </HBox.margin>
                                                                </Button>
                                                            </children>
                                                        </HBox>
                                                    </children>
                                                </VBox>
                                                <Label text="E-Mail:" GridPane.rowIndex="5">
                                                    <font>
                                                        <Font size="17.0"/>
                                                    </font>
                                                </Label>
                                                <Label fx:id="emailTextField" text="Label" GridPane.columnIndex="1"
                                                       GridPane.rowIndex="5"/>
                                            </children>
                                        </GridPane>
                                    </children>
                                </HBox>
                            </children>
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </VBox>
                        <LineChart fx:id="userChart" maxHeight="1.7976931348623157E308"
                                   maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                            <xAxis>
                                <CategoryAxis side="BOTTOM"/>
                            </xAxis>
                            <yAxis>
                                <NumberAxis side="LEFT"/>
                            </yAxis>
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </LineChart>
                    </children>
                    <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                    </padding>
                </HBox>
                <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0"
                      styleClass="mainBox" VBox.vgrow="ALWAYS">
                    <children>
                        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0"
                              spacing="10.0" HBox.hgrow="ALWAYS">
                            <children>
                                <WebView fx:id="webView" minHeight="-1.0" minWidth="-1.0" prefHeight="400.0"
                                         prefWidth="-1.0" VBox.vgrow="ALWAYS"/>
                            </children>
                        </VBox>
                    </children>
                    <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                    </padding>
                </HBox>
            </children>
        </VBox>
    </center>
    <bottom>
        <HBox prefHeight="64.0" prefWidth="1299.0" styleClass="footer" BorderPane.alignment="CENTER">
            <children>
                <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                      HBox.hgrow="ALWAYS">
                    <children>
                        <Region layoutX="640.0" layoutY="10.0" maxHeight="1.7976931348623157E308"
                                maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
                        <Region layoutX="1017.0" layoutY="10.0" maxHeight="1.7976931348623157E308"
                                maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
                        <Button contentDisplay="LEFT" maxHeight="1.7976931348623157E308"
                                maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#trainingClicked"
                                styleClass="trainingBtn" text="Zum Training" HBox.hgrow="ALWAYS">
                            <font>
                                <Font size="24.0"/>
                            </font>
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </Button>
                        <Region layoutX="10.0" layoutY="10.0" maxHeight="1.7976931348623157E308"
                                maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
                        <Region layoutX="11.0" layoutY="10.0" maxHeight="1.7976931348623157E308"
                                maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
                    </children>
                    <HBox.margin>
                        <Insets/>
                    </HBox.margin>
                </HBox>
            </children>
            <BorderPane.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </BorderPane.margin>
        </HBox>
    </bottom>
    <top>
        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="usernameLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                       text="Willkommen, Username!">
                    <font>
                        <Font size="24.0"/>
                    </font>
                </Label>
            </children>
            <BorderPane.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </BorderPane.margin>
        </VBox>
    </top>
</BorderPane>
