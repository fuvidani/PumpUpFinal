                <Label fx:id="musicPathLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                       style="-fx-font-size: 14;" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <font>
                        <Font size="14.0"/>
                    </font>
                    <GridPane.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                    </GridPane.margin>
                </Label>
            </children>
            <GridPane.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
