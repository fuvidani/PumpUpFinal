            <padding>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </padding>
        </ListView>
        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="To Do:" GridPane.rowIndex="2">
            <font>
                <Font size="24.0"/>
            </font>
            <GridPane.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </GridPane.margin>
        </Label>
        <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
              GridPane.columnSpan="2" GridPane.rowIndex="3">
            <children>
                <Region layoutX="363.0" layoutY="10.0" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
                <Button fx:id="startButton" alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308"
