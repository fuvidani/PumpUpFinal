                    <font>
                        <Font size="36.0"/>
                    </font>
                </Button>
                <Region maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
            </children>
            <GridPane.margin>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </GridPane.margin>
        </HBox>
    </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
    </padding>
    <stylesheets>
        <URL value="@../../css/WorkoutstartStyle.css"/>
